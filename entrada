// Enumerado
declaration type [
    id1 is {id2, id3} .
]

// Declaracao constante e variavel
1.
declaration constant and variable [
    as variable
        b1{3} is logic .
    as constant
        a1,a2 is string = 1 .
        a3,a4 is string = 2 .
]

2.
declaration constant and variable [
    as constant
        a1,a2 is string = 1 .
        a3,a4 is string = 2 .
    as variable
        b1{3} is logic .
]

// Atribuicao
designate this id1, id2, id3 as 837313

// Entrada de dados
read this [ id1, id2, id3, id4 ] .

// Saida de dados
write all this [id1, id2, id3, id4 ] .

